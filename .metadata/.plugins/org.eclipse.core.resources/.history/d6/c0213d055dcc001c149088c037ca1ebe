package com.chainsys.database;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.Statement;

public class Second {
	public static void main(String[] args) {
        try {
            Connection myCon = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:xe","system" ,"oracle" );
            Statement mystat = myCon.createStatement();
            ResultSet myRs = mystat.executeQuery("select * from employees");
            while (myRs.next()) {
                System.out.println(myRs.getInt(1) + " " + myRs.getString(2)+" "+myRs.getString(3)+" "+myRs.getString(4)+" "+myRs.getLong(1));
            }
            myCon.close();
        } catch (Exception err) {
            err.printStackTrace();
        }
    }

}
